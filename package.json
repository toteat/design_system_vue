{
  "name": "@toteat/design-system-vue",
  "version": "0.1.0",
  "description": "Vue 3 Design System with TypeScript support - Ships with modern ESNext syntax",
  "type": "module",
  "main": "dist/design-system-vue.umd.js",
  "module": "dist/design-system-vue.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/design-system-vue.js",
      "require": "./dist/design-system-vue.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "prepare": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "engineRequirements": [
    "Uses ESNext features that require transpilation in the consuming project"
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/vue3": "^8.6.12",
    "@storybook/vue3-vite": "^8.6.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.3",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.7.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "jsdom": "^24.1.3",
    "prettier": "^3.5.3",
    "storybook": "^8.6.12",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.6",
    "tailwindcss": "^4.1.6"
  }
}
